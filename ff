from django.core.mail import send_mail
from django.conf import settings

try:
    result = send_mail(
        'Test from Storm Cloud',
        'This is a test email.',
        settings.DEFAULT_FROM_EMAIL,
        ['smattymatt@gmail.com'],  # put your real email
        fail_silently=False
    )
    print('Sent:', result)
except Exception as e:
    print('Error:', e)
